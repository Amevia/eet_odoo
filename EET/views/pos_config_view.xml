<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="inherit_pos_config_form_view">
        <field name="name">inherit.pos.config.form.view</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="point_of_sale.pos_config_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//sheet/div[@id='title']" position="inside">
                <div class="row mt16 o_setting_container" id="estd_reg_no">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <label for="estd_reg_no" class="oe_edit_only"/>
                        <field name="estd_reg_no"/>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
